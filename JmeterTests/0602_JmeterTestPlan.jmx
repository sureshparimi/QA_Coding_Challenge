          <stringProp name="script">//double mean = prev.getTime() / prev.getSampleCount();
//double deviation = Math.sqrt(prev.getSquaredDeviation() / (prev.getSampleCount() - 1));
//if (Double.isNaN(deviation)) {
//    System.out.println(&quot;Standard deviation of response time: NaNms&quot;);
//} else {
//    System.out.println(&quot;Mean response time: &quot; + mean + &quot;ms&quot;);
//    System.out.println(&quot;Standard deviation of response time: &quot; + deviation + &quot;ms&quot;);
//}
List&lt;SampleResult&gt; samples = new ArrayList&lt;&gt;();

// Add each sample result to the list of samples
samples.add(prev);

double sum = 0;
double sumSquared = 0;

for (SampleResult sample : samples) {
    sum += sample.getTime();
    sumSquared += sample.getTime() * sample.getTime();
}

double mean = sum / samples.size();
double deviation;
if (samples.size() &gt; 1) {
    deviation = Math.sqrt((sumSquared - sum * sum / samples.size()) / (samples.size() - 1));
} else {
    deviation = Double.NaN;
}

System.out.println(&quot;-----------------------------------------------------------------&quot;);
System.out.println(&quot;                         SUMMARY REPORT&quot;);
System.out.println(&quot;-----------------------------------------------------------------&quot;);
System.out.println(&quot;Mean response time: &quot; + mean + &quot;ms&quot;);
if (Double.isNaN(deviation)) {
    System.out.println(&quot;Standard deviation of response time: Not available&quot;);
} else {
    System.out.println(&quot;Standard deviation of response time: &quot; + deviation + &quot;ms&quot;);
}
System.out.println(&quot;-----------------------------------------------------------------&quot;);
</stringProp>
